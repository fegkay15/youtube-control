{
  "manifest_version": 2,

  "name": "Youtube Control",
  "description": "Youtube listview",
  "version": "0.1",

  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Youtube Control"
  },
  "permissions": ["https://www.youtube.com/*"]
}
